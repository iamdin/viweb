{
	"name": "@viweb/server-trpc",
	"private": true,
	"type": "module",
	"version": "0.0.0",
	"description": "TRPC for Vibe Coding Devtools",
	"files": ["dist"],
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./routes/*": {
			"types": "./src/routes/*.ts",
			"import": "./dist/routes/*.js",
			"require": "./dist/routes/*.cjs"
		},
		"./agent": {
			"types": "./src/agent/index.ts",
			"import": "./dist/agent/index.js",
			"require": "./dist/agent/index.cjs"
		},
		"./orpc": {
			"types": "./src/orpc.ts",
			"import": "./dist/orpc.js",
			"require": "./dist/orpc.cjs"
		}
	},
	"typesVersions": {
		"*": {
			"*": ["./src/*"]
		}
	},
	"scripts": {
		"stub": "pnpm run build",
		"build": "tsdown src --format esm --format cjs --clean",
		"clean": "git clean -xdf node_modules dist .turbo"
	},
	"devDependencies": {
		"@types/ws": "^8.18.1",
		"tsdown": "^0.13.0",
		"@orpc/server": "^1.8.6",
		"@trpc/server": "^11.5.0",
		"ai": "^5.0.30",
		"ai-sdk-provider-claude-code": "^1.1.3",
		"superjson": "^2.2.2",
		"ws": "^8.18.3",
		"zod": "^3.25.67"
	},
	"peerDependencies": {
		"@orpc/server": "^1.8.6",
		"@trpc/server": "^11.5.0",
		"ai": "^5.0.30",
		"ai-sdk-provider-claude-code": "^1.1.3",
		"superjson": "^2.2.2",
		"ws": "^8.18.3",
		"zod": "^3.25.67"
	}
}

{
	"name": "@viweb/vscode-trpc",
	"private": true,
	"type": "module",
	"version": "0.0.0",
	"description": "TRPC for Vibe Coding Devtools",
	"files": ["dist"],
	"exports": {
		".": {
			"types": "./src/index.ts",
			"default": "./dist/index.cjs"
		},
		"./routes/*": {
			"types": "./src/routes/*.ts",
			"default": "./dist/routes/*.cjs"
		}
	},
	"typesVersions": {
		"*": {
			"*": ["./src/*"]
		}
	},
	"scripts": {
		"stub": "pnpm run build",
		"build": "tsdown src --external vscode --format cjs --shims",
		"clean": "git clean -xdf node_modules dist .turbo"
	},
	"dependencies": {
		"@trpc/server": "^11.5.0",
		"superjson": "^2.2.2",
		"ws": "^8.18.3",
		"zod": "^3.25.67"
	},
	"devDependencies": {
		"@types/ws": "^8.18.1",
		"tsdown": "^0.13.0"
	}
}

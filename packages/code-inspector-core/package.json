{
	"name": "@viweb/code-inspector-core",
	"version": "0.0.2",
	"type": "module",
	"keywords": [
		"inspector"
	],
	"files": [
		"dist"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.cts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./babel": {
			"import": "./dist/babel.js",
			"require": "./dist/babel.cjs"
		},
		"./constants": {
			"import": "./dist/constants.js",
			"require": "./dist/constants.cjs"
		},
		"./transform": {
			"import": "./dist/transform.js",
			"require": "./dist/transform.cjs"
		},
		"./visitor": {
			"import": "./dist/visitor.js",
			"require": "./dist/visitor.cjs"
		},
		"./*": "./*"
	},
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./*"
			]
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"stub": "tsdown",
		"test": "vitest run",
		"build": "tsdown",
		"typecheck": "tsc --noEmit",
		"clean": "git clean -xdf node_modules dist .turbo"
	},
	"dependencies": {
		"@babel/core": "^7.28.3",
		"@babel/types": "^7.28.2",
		"@vue/babel-plugin-jsx": "^1.5.0",
		"@vue/compiler-dom": "^3.5.19",
		"@vue/compiler-sfc": "^3.5.19",
		"magic-string": "^0.30.18",
		"pathe": "^2.0.3",
		"tiny-invariant": "^1.3.3"
	},
	"devDependencies": {
		"@types/babel__core": "^7.20.5",
		"@types/babel__traverse": "^7.28.0",
		"publint": "^0.3.12",
		"tsdown": "^0.13.0",
		"vitest": "^3.2.4"
	}
}

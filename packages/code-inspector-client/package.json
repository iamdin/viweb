{
	"name": "@viweb/code-inspector-client",
	"version": "0.0.2",
	"type": "module",
	"description": "code inspector client code",
	"keywords": ["inspector"],
	"scripts": {
		"stub": "vite build --mode development && tsdown",
		"build": "vite build && tsdown",
		"typecheck": "tsc --noEmit",
		"clean": "git clean -xdf node_modules dist .turbo"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"default": "./dist/index.js"
		},
		"./*": "./*"
	},
	"typesVersions": {
		"*": {
			"*": ["./dist/*", "./*"]
		}
	},
	"devDependencies": {
		"@floating-ui/react": "^0.27.16",
		"@tailwindcss/vite": "^4.1.12",
		"@types/react": "^19.1.11",
		"@types/react-dom": "^19.1.9",
		"@vitejs/plugin-react-oxc": "^0.4.1",
		"@vitejs/plugin-react-swc": "^4.0.1",
		"@viweb/code-inspector-web": "workspace:*",
		"@xstate/react": "^6.0.0",
		"birpc": "^2.5.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lucide-react": "^0.530.0",
		"pathe": "^2.0.3",
		"publint": "^0.3.12",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.12",
		"tiny-invariant": "^1.3.3",
		"tsdown": "^0.13.0",
		"tw-animate-css": "^1.3.8",
		"vite": "npm:rolldown-vite@latest",
		"xstate": "^5.20.2"
	}
}
